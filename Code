CREATE TABLE movie (
    id              INTEGER      PRIMARY KEY AUTOINCREMENT
                                 NOT NULL,
    Movie_Name      VARCHAR (40),
    Actor_Name      CHAR (30),
    Actress_Name    CHAR (30),
    Director        CHAR (30),
    Year_Of_Release INTEGER (4) 
);


SELECT * from movie;

SELECT Movie_Name,Actor_Name from movie where Actor_Name="Nicolas Cage";
